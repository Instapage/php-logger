{
    "name": "postclick/php-logger",
    "type": "library",
    "description": "PHP logger",
    "version": "1.0.0",
    "require": {
        "php": "^7.4.0 || ^8.0",
        "ext-json": "*",
        "psr/log": "^1.1",
        "monolog/monolog": "~1.22 || ~2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Postclick\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\\unit\\src"
        }
    }
}
